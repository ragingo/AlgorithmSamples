{
    "configurations": [
        {
            "name": "Win32",
            "compilerPath": "cl.exe",
            "intelliSenseMode": "msvc-x64",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "cStandard": "c23",
            "cppStandard": "c++26",
            "browse": {
                "path": ["${workspaceFolder}/**"],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
            }
        }
    ],
    "version": 4
}